<h1>New restaurant</h1>


<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <%= simple_form_for @restaurant do |f| %>
        <%= f.input :name %>
        <%= f.input :address %>
        <%= f.input :phone_number %>
        <%= f.input :category %>
        <%= f.submit class: "btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>
